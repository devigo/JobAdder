@using JobAdder.Integrations.Models;
@model IEnumerable<Candidate>
@{
    ViewData["Title"] = "List of Candidates";
}
<h3>@ViewData["Title"]</h3>
<div id="content" class="container">
    <div class="row">
        <div class="col-sm-4 col-xs-5"><label>Name</label></div>
        <div class="col-sm-8 col-xs-7"><label>Skill Tags</label></div>
    </div>
    @foreach (Candidate item in Model)
    {
        <div class="row">
            <div class="col-sm-4 col-xs-5">@item.Name</div>
            <div class="col-sm-8 col-xs-7">@item.SkillTags</div>
        </div>
    }
</div>